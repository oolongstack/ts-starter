{
  "name": "@amoo/ts-starter",
  "version": "1.0.1",
  "description": "ðŸŒž A minimist starter template for typescript library",
  "type": "module",
  "keywords": [
    "template",
    "starter",
    "typescript"
  ],
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vitest",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "build": "tsup",
    "lint": "tsc",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "format": "prettier --write .",
    "test": "vitest run",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/oolongstack/ts-starter",
  "bugs": {
    "url": "https://github.com/oolongstack/ts-starter/issues"
  },
  "author": "amoo <oolong.stack@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:oolongstack/ts-starter.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@changesets/cli": "^2.28.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
